/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=notsel);
    And(a = a, b= notsel, out = temp1);
    And(a = b, b = sel, out = temp2);
    Or(a = temp1, b = temp2, out = out);
}